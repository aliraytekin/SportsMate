<h1>Events#index</h1>
<p>Find me in app/views/events/index.html.erb</p>

<div class="container">
  <% @events.each do |event| %>
  <%= event.title %>
  <%= event.venue %>
  <% end %>
</div>

<div class="container">
<div data-controller="map"
    data-map-markers-value="<%= @markers.to_json %>"
    data-map-api-key-value="<%= ENV['MAPBOX_API_KEY'] %>"
    style='width: 100%; height: 300px;'>
  </div>
</div>
